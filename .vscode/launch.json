  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "development",
      "cwd": "${workspaceFolder}",
      "runtimeVersion": "18.16.0",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:dev"],
      "env": {
        "NODE_ENV": "development",
        "DB_NAME": "nest-crud-db-2",
        "DB_HOST": "localhost",
        "DB_PORT": 5432,
        "DB_USERNAME": "postgres",
        "DB_PASSWORD": "postgres",
        "DB_SYNC": true
      },
      "outputCapture": "std"
    }
  ]
}
